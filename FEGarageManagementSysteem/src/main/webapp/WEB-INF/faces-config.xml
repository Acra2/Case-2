<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>customerIndex</from-outcome>
            <to-view-id>/customerIndex.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>customerCreate</from-outcome>
            <to-view-id>/customerCreate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>updateCustomer</from-outcome>
            <to-view-id>/customerUpdate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>car</from-outcome>
            <to-view-id>/car.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>updateCar</from-outcome>
            <to-view-id>/update_car.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>customerOverview</from-outcome>
            <to-view-id>/customer_overview.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>details</from-outcome>
            <to-view-id>/details_car.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>maintenance</from-outcome>
            <to-view-id>/maintenance.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>inMaintenance</from-outcome>
            <to-view-id>/inMaintenance.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>maintenanceTypes</from-outcome>
            <to-view-id>/maintenaceType.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>updateMaintenanceType</from-outcome>
            <to-view-id>/update_maintenancetype.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>maintenanceDetails</from-outcome>
            <to-view-id>/details_maintenance.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>